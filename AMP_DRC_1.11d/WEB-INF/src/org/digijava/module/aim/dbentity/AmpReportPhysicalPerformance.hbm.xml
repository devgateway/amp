<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpReportPhysicalPerformance" table="AMP_REPORT_PHYSICAL_PERFORMANCE">
		<cache usage="read-write"/>
		
		<id name="ampPpId" column="amp_pp_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_REPORT_PHYSICAL_PERFORMANCE_seq</param>
			</generator>
		</id>
		<property column="amp_activity_id" name="ampActivityId" type="java.lang.Long"/>
		<property column="title" name="title" type="java.lang.String"/>
		<property column="description" name="description" type="org.digijava.kernel.db.SafeClobType" />
		
	</class> 
</hibernate-mapping>