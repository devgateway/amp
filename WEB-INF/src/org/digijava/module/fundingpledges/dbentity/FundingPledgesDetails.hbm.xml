<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.fundingpledges.dbentity.FundingPledgesDetails" table="amp_funding_pledges_details">
		<id name="Id" column="id" type="java.lang.Long"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">amp_funding_pledges_details_seq</param>
			</generator>
		</id>
		<property name="funding_date" column="funding_date" type="java.sql.Timestamp"/>
		<property name="amount" column="amount" type="java.lang.Double"/>
		<property name="pledgetype" column="pledge_type" type="java.lang.Integer"/>
		
		<many-to-one name="pledgeid" column="pledge_id" class="org.digijava.module.fundingpledges.dbentity.FundingPledges" cascade="none" not-null="true" />
		<many-to-one name="currency" column="currency" class="org.digijava.module.aim.dbentity.AmpCurrency" cascade="none" />
		<many-to-one name="typeOfAssistance" column="type_of_assistance" class="org.digijava.module.categorymanager.dbentity.AmpCategoryValue" cascade="none" />
		<many-to-one name="aidmodality" column="aid_modality" class="org.digijava.module.categorymanager.dbentity.AmpCategoryValue" cascade="none" />
	</class>
</hibernate-mapping>