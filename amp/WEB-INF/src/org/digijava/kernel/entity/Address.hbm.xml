<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.kernel.entity.Address" table="EP_ADDRESSES"
		proxy="org.digijava.kernel.entity.Address">
		<id column="ADDRESS_ID" name="addressId" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">EP_ADDRESSES_seq</param>
			</generator>
		</id>
		<!--property column="AGENCY_ID" length="22" name="agencyId" type="java.lang.Long"//-->
		<property column="ADDRESS" length="400" name="address" type="java.lang.String" />
		<property column="ADDRESS2" length="400" name="address2" type="java.lang.String" />
		<property column="CITY" length="200" name="city" type="java.lang.String" />
		<property column="STATE" length="200" name="state" type="java.lang.String" />
		<property column="COUNTRY" length="200" name="country" type="java.lang.String" />
		<property column="COUNTRY_CODE" length="2" name="countryCode" type="java.lang.String" />
		<property column="POSTAL_CODE" length="100" name="postalCode" type="java.lang.String" />
		<property column="FIRST_NAMES" length="200" name="firstNames" type="java.lang.String" />
		<property column="LAST_NAME" length="200" name="lastName" type="java.lang.String" />
		<property column="TITLE" length="200" name="title" type="java.lang.String" />
		<property column="DEPARTMENT" length="200" name="department" type="java.lang.String" />
		<property column="PHONE" length="100" name="phone" type="java.lang.String" />
		<property column="FAX" length="100" name="fax" type="java.lang.String" />
		<property column="EMAIL" length="100" name="email" type="java.lang.String" />
		<property column="URL" length="300" name="url" type="java.lang.String" />
		<property column="OLD_ADDRESS_ID" length="22" name="oldAddressId" type="java.lang.Long" />
	</class>
</hibernate-mapping>
