<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.digijava.kernel.entity.integration.FileInformation" table="dg_file_information" schema="etl">
        <id name="id" type="java.lang.Long" >
            <column name="id" />
            <generator class="native">
                <param name="sequence_name">dg_file_information_seq</param>
            </generator>
        </id>
        <property name="fileName" column="file_name" type="java.lang.String" />
        <property name="records" column="records" type="java.lang.Integer" />
        <property name="success" column="success" type="java.lang.Integer" />
        <property name="error" column="error" type="java.lang.Integer" />
        <property name="status" column="status" type="java.lang.String" />
        <property name="createdDate" column="created_date" type="java.util.Date" />
        <set name="fileRecords" cascade="all" lazy="true">
            <cache usage="read-write" />
            <key column="file_information_id"/>
            <one-to-many class="org.digijava.kernel.entity.integration.FileRecord" />
        </set>
    </class>
</hibernate-mapping>
