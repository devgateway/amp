<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.digijava.kernel.entity.integration.FileRecord" table="dg_file_record" schema="etl">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native">
                <param name="sequence_name">dg_file_record_seq</param>
            </generator>
        </id>

        <many-to-one name="fileInformation" class="org.digijava.kernel.entity.integration.FileInformation" fetch="join">
            <column name="file_information_id" not-null="true" />
        </many-to-one>

        <property name="content" type="java.lang.String" column="content"/>

        <property name="status" type="java.lang.String" column="status"/>

        <property name="error" type="java.lang.String" column="error"/>

        <property name="ampId" type="java.lang.String" column="amp_id"/>

        <property name="row" type="java.lang.Integer" column="row"/>

        <property name="createdDate" type="java.util.Date" column="created_date"/>
    </class>
</hibernate-mapping>