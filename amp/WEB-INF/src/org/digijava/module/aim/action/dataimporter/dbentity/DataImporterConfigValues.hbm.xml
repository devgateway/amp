<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.digijava.module.aim.action.dataimporter.dbentity.DataImporterConfigValues" table="DATA_IMPORTER_CONFIG_VALUES">
        <cache usage="read-write" />
        <id name="configKey" column="config_key" type="string">
            <generator class="assigned" />
        </id>
        <property name="configValue" column="config_value" type="string"/>
        <many-to-one name="dataImporterConfig" column="data_importer_config_id" class="org.digijava.module.aim.action.dataimporter.dbentity.DataImporterConfig" not-null="true" />


    </class>
</hibernate-mapping>
