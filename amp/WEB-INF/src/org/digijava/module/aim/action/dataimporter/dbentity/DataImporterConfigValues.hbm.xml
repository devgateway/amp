<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.digijava.module.aim.action.dataimporter.dbentity.DataImporterConfigValues" table="DATA_IMPORTER_CONFIG_VALUES">
        <cache usage="read-write" />
        <id name="id" column="id" type="java.lang.Long">
            <generator class="native">
                <param name="sequence">DATA_IMPORTER_CONFIG_VALUES_SEQ</param>
            </generator>
        </id>
        <property name="configKey" column="config_key" type="string"/>
        <property name="configValue" column="config_value" type="string"/>


    </class>
</hibernate-mapping>
