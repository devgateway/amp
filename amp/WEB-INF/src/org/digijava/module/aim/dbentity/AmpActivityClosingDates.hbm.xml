<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpActivityClosingDates"
		table="AMP_ACTIVITY_CLOSING_DATES">
		<cache usage="read-write" />
		<id name="ampActivityClosingDateId" column="amp_activity_closing_date_id"
			type="java.lang.Long">
			<generator class="native">
				<param name="sequence">amp_activity_closing_dates_seq</param>
			</generator>
		</id>

		<property name="type" column="type" type="java.lang.Integer" />
		<property name="closingDate" column="closing_date" type="java.util.Date" />
		<property name="comments" column="comments" type="text" />

		<many-to-one name="ampActivityId" column="amp_activity_id"
			class="org.digijava.module.aim.dbentity.AmpActivityVersion" not-null="true" />
	</class>
</hibernate-mapping>
