<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.digijava.module.aim.dbentity.AmpActivityIndirectSector" table="AMP_ACTIVITY_INDIRECT_SECTOR">

		<cache usage="read-write" />

		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_ACTIVITY_INDIRECT_SECTOR_SEQ</param>
			</generator>
		</id>

		<many-to-one name="activitySector" column="amp_activity_sector_id"
			class="org.digijava.module.aim.dbentity.AmpActivitySector" cascade="none" not-null="true" />

		<many-to-one name="sector" column="amp_sector_id"
			class="org.digijava.module.aim.dbentity.AmpSector" cascade="none" not-null="true" lazy="false" />

		<property name="percentage" column="sector_percentage" type="java.math.BigDecimal" precision="5" scale="2" />

	</class>

</hibernate-mapping>
