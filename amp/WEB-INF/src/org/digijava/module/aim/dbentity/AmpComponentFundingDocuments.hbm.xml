<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.digijava.module.aim.dbentity.AmpComponentFundingDocuments" table="amp_component_funding_documents">
        <id name="id" column="id" type="java.lang.Long" unsaved-value="null">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence_name">amp_component_funding_documents_seq</param>
            </generator>
        </id>
        <property name="fileName" column="file_name" type="java.lang.String"/>
        <property name="fileBase64String" column="file_base_64_string" type="text"/>
        <many-to-one column="amp_component_funding_id" name="ampComponentFunding" class="org.digijava.module.aim.dbentity.AmpComponentFunding" />


    </class>
</hibernate-mapping>