<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.digijava.module.aim.dbentity.AmpIndicatorLocation"
           table="AMP_INDICATOR_LOCATION">
        <cache usage="read-write" />
        <id name="ampIndLocId" column="amp_ind_loc_id" type="java.lang.Long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence_name">AMP_INDICATOR_LOCATION_seq</param>
            </generator>
        </id>

        <many-to-one name="indicator" column="indicator_id"
                     class="org.digijava.module.aim.dbentity.AmpIndicator" cascade="none" lazy="false"/>

        <many-to-one name="location" column="location_id"
                     class="org.digijava.module.aim.dbentity.AmpCategoryValueLocations" cascade="none" lazy="false"/>

    </class>

</hibernate-mapping>
