<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpSectorMapping" table="AMP_SECTOR_MAPPING" lazy="false">

		<cache usage="read-write" />

		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_SECTOR_MAPPING_SEQ</param>
			</generator>
		</id>

		<many-to-one name="srcSector" column="src_sector_id"
					 class="org.digijava.module.aim.dbentity.AmpSector"
					 cascade="none" not-null="true" unique-key='uniqueSectorMapping' />

		<many-to-one name="dstSector" column="dst_sector_id"
					 class="org.digijava.module.aim.dbentity.AmpSector"
					 cascade="none" not-null="true" unique-key='uniqueSectorMapping' />

	</class>
</hibernate-mapping>
