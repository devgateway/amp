<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpTeamMemberRoles"
		table="AMP_TEAM_MEMBER_ROLES" lazy="false">
		<cache usage="read-write" />
		<id name="ampTeamMemRoleId" column="amp_team_mem_role_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_TEAMMEMROLE_seq</param>
			</generator>
		</id>

		<property name="role" column="role" type="java.lang.String" />
		<property name="description" column="description" type="text" />
		<property name="readPermission" column="read_permission" type="java.lang.Boolean" />
		<property name="writePermission" column="write_permission" type="java.lang.Boolean" />
		<property name="deletePermission" column="delete_permission" type="java.lang.Boolean" />
		<property name="teamHead" column="team_head" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>
