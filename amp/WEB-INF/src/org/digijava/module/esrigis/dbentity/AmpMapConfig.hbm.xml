<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.esrigis.dbentity.AmpMapConfig" table="AMP_MAP_CONFIG">
		<id name="Id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">amp_map_config_seq</param>
			</generator>
		</id>

		<property name="mapUrl" column="mapurl" type="java.lang.String" />
		<property name="mapType" column="maptype" type="java.lang.Integer" />
		<property name="countyField" column="county_field" type="java.lang.String" />
		<property name="districtField" column="district_field" type="java.lang.String" />
		<property name="geoIdField" column="geoid_field" type="java.lang.String" />
		<property name="countField" column="count_field" type="java.lang.String" />
		<property name="mapSubType" column="map_subtype" type="java.lang.Integer" />
		<property name="legendImage" column="legend_image" type="binary" length="500000"/>
		<property name="configName" column="config_name" type="java.lang.String" />
		<property name="legendNotes" column="legend_notes" type="text" />



	</class>
</hibernate-mapping>