<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.gis.dbentity.GisMapShape" table="DG_GIS_MAP_SHAPE">
		<cache usage="read-write" />
		<id column="ID" name="id" type="long" unsaved-value="0">
			<generator class="native">
				<param name="sequence">dg_gis_map_shape_seq</param>
			</generator>
		</id>
		<many-to-one name="segment" column="SEGMENT"
			class="org.digijava.module.gis.dbentity.GisMapSegment" cascade="none" />

		<bag lazy="false" name="shapePoints" cascade="all" inverse="true" order-by="SEQ_NO asc">
			<key column="SHAPE" />
			<one-to-many class="org.digijava.module.gis.dbentity.GisMapPoint" />
		</bag>
		<property name="shapeId" column="SHAPE_ID" type="long" />
	</class>
</hibernate-mapping>
