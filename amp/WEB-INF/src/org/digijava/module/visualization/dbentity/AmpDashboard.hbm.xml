<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.visualization.dbentity.AmpDashboard" table="amp_dashboard">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">amp_dashboard_seq</param>
			</generator>
		</id>
	
		<property name="name" column="name" type="java.lang.String" />
		<property name="baseType" column="base_type" type="java.lang.Integer" />
		<property name="pivot" column="pivot" type="java.lang.Integer" />
		<property name="showInMenu" column="show_in_menu" type="java.lang.Boolean" />
		<property name="showAcronymForOrgNames" column="show_acronym_for_org_names" type="java.lang.Boolean" />
		<property name="maxYearFilter" column="max_year_filter" type="java.lang.Integer" not-null="false"/>
		<property name="minYearFilter" column="min_year_filter" type="java.lang.Integer" not-null="false"/>
		<property name="transactionTypeFilter" column="transaction_type_filter" type="java.lang.Integer" not-null="false"/>
		
 	 </class>
</hibernate-mapping>