<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<form-beans>
		<form-bean name="beBudgetExportForm" type="org.digijava.ampModule.budgetexport.form.BudgetExportForm" />
		<form-bean name="beReportsWrapperForm" type="org.digijava.ampModule.budgetexport.form.ReportsWrapperForm" />
		<form-bean name="beMapRuleForm" type="org.digijava.ampModule.budgetexport.form.BEMapRuleForm" />
		<form-bean name="beMapingForm" type="org.digijava.ampModule.budgetexport.form.BEMappingForm" />
		<form-bean name="beProjectForm" type="org.digijava.ampModule.budgetexport.form.BEProjectForm" />
		<form-bean name="beMapActionsForm" type="org.digijava.ampModule.budgetexport.form.BEMapActionsForm" />
	</form-beans>
	<action-mappings>
		<action name="beBudgetExportForm" path="/budgetexport/budgetExport" scope="request" 
						type="org.digijava.ampModule.budgetexport.action.BudgetExportAction" validate="false">
			<forward name="forward" path="/showLayout.do?layout=viewBudgetExportManager" />
		</action>
		<action name="beReportsWrapperForm" path="/budgetexport/showBudgetReport" scope="request" 
						type="org.digijava.ampModule.budgetexport.action.ReportsWrapperAction" validate="false">
			<forward name="forward" path="/showLayout.do?layout=reportWrapper" />
		</action>

		<action forward="/budgetexport/showProjectList.do" path="/budgetexport/index" />

		<action  name="beBudgetExportForm" path="/budgetexport/showProjectList" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.ShowProjectList" validate="false">
			<forward name="forward" path="/showLayout.do?layout=viewBudgetExportManager"/>
		</action>

		<action  name="beProjectForm" path="/budgetexport/addEditDeleteProject" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.AddEditDeleteProject" validate="false" parameter="action">
			<forward name="forward" path="/showLayout.do?layout=addEditBudgetExpProject" />
			<forward name="projectList" path="/budgetexport/showProjectList.do" redirect="true"/>
		</action>
		
		<action  name="beMapingForm" path="/budgetexport/showMapingRuleList" scope="request" 
				type="org.digijava.ampModule.budgetexport.action.ShowMapingRuleList" validate="false" parameter="action">
			<forward name="forward" path="/showLayout.do?layout=viewMappingRuleList" />
		</action>

		<action  name="beMapRuleForm" path="/budgetexport/addEditDeleteMapRule" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.AddEditDeleteMapRule" validate="false" parameter="action">
			<forward name="forward" path="/showLayout.do?layout=addEditMapRule" />
			<forward name="ruleList" path="budgetexport/showMapingRuleList.do"/>
		</action>
		<action  name="beBudgetExportForm" path="/budgetexport/budgetExportService" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.BudgetExportService" validate="false"/>


		<action  name="beMapActionsForm" path="/budgetexport/mapActions" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.MapActions" validate="false" parameter="action">
			<forward name="forward" path="/showLayout.do?layout=budgetExportMapActions" />
			<forward name="projectRules" path="budgetexport/showMapingRuleList.do"/>
		</action>

		<action  name="beBudgetExportForm" path="/budgetexport/exportMapping" 
				scope="request" type="org.digijava.ampModule.budgetexport.action.ExportMapping" validate="false"/>

		


	</action-mappings>
</struts-config>

