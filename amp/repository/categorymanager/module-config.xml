<?xml version="1.0"?>
<module-config>
	<name>categorymanager</name>


	<security login-required="true" default="READ">
		<action login-required="true" pattern="/categoryManager">ADMIN</action>
	</security>

	<hibernate-classes required="true">
		<hibernate-class>
			org.digijava.module.categorymanager.dbentity.AmpCategoryClass
		</hibernate-class>
		<hibernate-class>
			org.digijava.module.categorymanager.dbentity.AmpCategoryValue
		</hibernate-class>
		<hibernate-class>
			org.digijava.module.categorymanager.dbentity.AmpLinkedCategoriesState
		</hibernate-class>
	</hibernate-classes>
</module-config>
