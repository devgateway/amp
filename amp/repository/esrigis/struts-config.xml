<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="mainmapform" type="org.digijava.module.esrigis.form.MainMapForm"/>
		<form-bean name="maphelpform" type="org.digijava.module.esrigis.form.MapHelpForm"/>
	</form-beans>
	<action-mappings>
		<action name="mainmapform" path="/esrigis/mainmap" scope="session" type="org.digijava.module.esrigis.action.MainMap">
			<forward name="forward" path="/showLayout.do?layout=mainmap" />
		</action>
		<action name="maphelpform" path="/esrigis/maphelper" scope="session" validate="false" type="org.digijava.module.esrigis.action.MapHelper"></action>
	</action-mappings>
</struts-config>
