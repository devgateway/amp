<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.dgfoundation.amp.ar.dbentity.AmpTeamFilterData" table="AMP_TEAM_FILTER_DATA">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_TEAM_FILTER_DATA_seq</param>
			</generator> 
		</id>
		<property name="propertyName" column="property_name"  type="java.lang.String"/>
		<property name="propertyClassName" column="property_class_name"  type="java.lang.String"/>
		<property name="elementClassName" column="element_class_name"  type="java.lang.String"/>
		<property name="value" column="value"  type="java.lang.String"/>
		<many-to-one name="filterRelObj" column="amp_team_id" class="org.digijava.ampModule.aim.dbentity.AmpTeam" not-null="true"/>
	</class>
</hibernate-mapping>