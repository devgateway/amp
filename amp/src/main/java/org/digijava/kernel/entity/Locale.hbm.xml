<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.kernel.entity.Locale" table="DG_LOCALE" lazy="false">
		<cache usage="read-write" />
		<id name="code" column="CODE" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property column="NAME" name="name" type="java.lang.String" />
		<property column="AVAILABLE" name="available" type="boolean" />
		<property column="MESSAGE_LANG_KEY" name="messageLangKey" type="java.lang.String" />
		<property column="LEFT_TO_RIGHT" name="leftToRight" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>
