<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- org.digijava.kernel.request.SiteDomainMap root -->
	<class name="org.digijava.kernel.request.SiteDomainMap" table="SiteDomainMap" mutable="false">
		<id name="id" type="long" column="id">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">SiteDomainMap_seq</param>
			</generator>
		</id>
		<property name="domain" column="domain" type="string" />
		<property name="siteId" column="site" type="string" />
	</class>
</hibernate-mapping>
