<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="locale">		<xs:complexType>			<xs:sequence>				<xs:element name="months">					<xs:complexType>						<xs:sequence>							<xs:element name="month" type="xs:string" minOccurs="12" maxOccurs="12"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="short-months">					<xs:complexType>						<xs:sequence>							<xs:element name="short-month" type="xs:string" minOccurs="12" maxOccurs="12"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="days">					<xs:complexType>						<xs:sequence>							<xs:element name="day" type="xs:string" minOccurs="7" maxOccurs="7"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="short-days">					<xs:complexType>						<xs:sequence>							<xs:element name="short-day" type="xs:string" minOccurs="7" maxOccurs="7"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="eras">					<xs:complexType>						<xs:sequence>							<xs:element name="era" type="xs:string" minOccurs="2" maxOccurs="2"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="ampms">					<xs:complexType>						<xs:sequence>							<xs:element name="ampm" type="xs:string" minOccurs="2" maxOccurs="2"/>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="date-patterns">					<xs:complexType>						<xs:sequence>							<xs:element name="date-pattern" type="xs:string" minOccurs="4" maxOccurs="unbounded"/>						</xs:sequence>						<xs:attribute name="default" type="xs:nonNegativeInteger" use="required"/>					</xs:complexType>				</xs:element>				<xs:element name="time-patterns">					<xs:complexType>						<xs:sequence>							<xs:element name="time-pattern" type="xs:string" minOccurs="4" maxOccurs="12"/>						</xs:sequence>						<xs:attribute name="default" type="xs:nonNegativeInteger" use="required"/>					</xs:complexType>				</xs:element>				<xs:element name="date-time-pattern" type="xs:string"/>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>