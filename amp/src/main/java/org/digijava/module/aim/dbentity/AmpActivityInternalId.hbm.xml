<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.ampModule.aim.dbentity.AmpActivityInternalId"
		table="AMP_ACTIVITY_INTERNAL_ID">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_ACTIVITY_INTERNAL_ID_seq</param>
			</generator>
		</id>

		<many-to-one name="ampActivity" column="amp_activity_id"
			class="org.digijava.ampModule.aim.dbentity.AmpActivityVersion" cascade="none" not-null="true" />

		<many-to-one name="organisation" column="amp_org_id"
			class="org.digijava.ampModule.aim.dbentity.AmpOrganisation" cascade="merge" not-null="true" lazy="false"/>

		<property name="internalId" column="internal_id" type="java.lang.String" />

	</class>
</hibernate-mapping>
