<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpChapter" table="AMP_CHAPTER">
		<cache usage="read-write" />
		<id name="code" column="code" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="year" column="year" type="java.lang.Integer" />
		<property name="description" column="description" type="java.lang.String"
			length="255" />

		<set name="imputations" lazy="false" inverse="false" cascade="all-delete-orphan">
			<cache usage="read-write" />
			<key column="chapter_code" />
			<one-to-many class="org.digijava.ampModule.aim.dbentity.AmpImputation" />
		</set>

		
	</class>
</hibernate-mapping>