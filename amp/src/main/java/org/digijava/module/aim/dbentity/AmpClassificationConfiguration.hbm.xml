<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpClassificationConfiguration"
		table="AMP_CLASSIFICATION_CONFIG">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_CLASSIFICATION_CONFIG_seq</param>
			</generator>
		</id>

		<property name="name" column="name" type="java.lang.String" />
		<property name="description" column="description" type="java.lang.String" />
		<property name="multisector" column="multisector" type="boolean" />
		<property name="primary" column="is_primary_sector" type="boolean" />

		<many-to-one name="classification" column="classification_id"
			class="org.digijava.ampModule.aim.dbentity.AmpSectorScheme" cascade="none" lazy="false"/>

	</class>
</hibernate-mapping>