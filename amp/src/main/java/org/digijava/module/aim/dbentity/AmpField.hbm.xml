<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpField" table="AMP_FIELD">
		<cache usage="read-write" />
		<id name="ampFieldId" column="amp_field_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_FIELD_seq</param>
			</generator>
		</id>
		<property name="fieldName" column="field_name" type="java.lang.String" />
	</class>
</hibernate-mapping>
