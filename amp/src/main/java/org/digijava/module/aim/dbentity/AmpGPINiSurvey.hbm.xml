<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpGPINiSurvey"
		table="AMP_GPI_NI_SURVEY">
		<cache usage="read-write" />
		<id name="ampGPINiSurveyId" column="amp_gpi_ni_survey_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_GPI_NI_SURVEY_seq</param>
			</generator>
		</id>			
		<property name="surveyDate" column="survey_date" type="java.util.Date" />		
		<many-to-one name="ampOrgRole" column="amp_org_role_id" class="org.digijava.ampModule.aim.dbentity.AmpOrgRole"
			unique="true" />		
			<set name="responses" table="AMP_GPI_NI_SURVEY_RESPONSE" inverse="true"
			cascade="all">
			<cache usage="read-write" />
			<key column="amp_gpi_ni_survey_id" />
			<one-to-many class="org.digijava.ampModule.aim.dbentity.AmpGPINiSurveyResponse" />
		</set>

	</class>
</hibernate-mapping>