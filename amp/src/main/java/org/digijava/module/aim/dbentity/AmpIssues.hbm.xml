<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpIssues" table="AMP_ISSUES">
		<cache usage="read-write" />
		<id name="ampIssueId" column="amp_issue_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_ISSUES_seq</param>
			</generator>
		</id>
		<property column="name" name="name" type="text" />
		<property column="issueDate" name="issueDate" type="java.util.Date" />
		<many-to-one name="activity" column="amp_activity_id"
			class="org.digijava.ampModule.aim.dbentity.AmpActivityVersion" cascade="delete" />
		<set name="measures" cascade="all" inverse="true" order-by="amp_measure_id asc">
			<cache usage="read-write" />
			<key column="amp_issue_id" />
			<one-to-many class="org.digijava.ampModule.aim.dbentity.AmpMeasure" />
		</set>
	</class>
</hibernate-mapping>
