<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpLuceneIndexStamp" table="AMP_LUCENE_INDEX">
		<cache usage="read-write" />
		<id name="idxId" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_LUCENE_INDEX_seq</param>
			</generator>
		</id>
		<property name="idxName" column="idxName" type="java.lang.String" />
		<property name="stamp" column="stamp" type="java.lang.Long" />
	</class>
</hibernate-mapping>
