<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpMEIndicators" table="AMP_ME_INDICATORS">
		<cache usage="read-write" />
		<id name="ampMEIndId" column="amp_me_indicator_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_ME_INDICATORS_seq</param>
			</generator>
		</id>

		<property name="code" column="code" type="java.lang.String" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="description" column="description" type="text" />
		<property name="defaultInd" column="default_ind" type="java.lang.Boolean" />
		<property name="ascendingInd" column="ASCENDING_IND" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>
