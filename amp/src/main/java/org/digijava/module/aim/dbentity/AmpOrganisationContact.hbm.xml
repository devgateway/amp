<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpOrganisationContact" table="AMP_ORG_CONTACT" lazy="false">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_ORG_CONTACT_seq</param>
			</generator>
		</id>
	
	<property name="primaryContact" column="is_primary_contact" type="java.lang.Boolean" />
	
	<many-to-one name="organisation" column="amp_org_id"	class="org.digijava.ampModule.aim.dbentity.AmpOrganisation" cascade="none" lazy="false"/>
	<many-to-one name="contact"  column="contact_id"	class="org.digijava.ampModule.aim.dbentity.AmpContact" cascade="none" lazy="false"/>
	</class>	
</hibernate-mapping>
