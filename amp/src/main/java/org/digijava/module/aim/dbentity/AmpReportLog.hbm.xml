<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.digijava.ampModule.aim.dbentity.AmpReportLog" table="AMP_REPORT_LOG">
		<cache usage="read-write" />
		<id name="ampReportMemberLogId" column="amp_reports_member_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" >
			<param name="sequence_name">amp_report_log_seq</param>
			</generator>
		</id>

		<many-to-one name="report" column="amp_report_id"
			class="org.digijava.ampModule.aim.dbentity.AmpReports" cascade="none" />

		<many-to-one name="member" column="amp_member_id"
			class="org.digijava.ampModule.aim.dbentity.AmpTeamMember" cascade="none" />

		<property name="lastView" column="last_time_shown" type="java.util.Date" />

	</class>

</hibernate-mapping>
