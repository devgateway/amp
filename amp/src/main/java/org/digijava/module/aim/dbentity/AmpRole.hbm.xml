<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpRole" table="AMP_ROLE">
		<cache usage="read-write" />
		<id name="ampRoleId" column="amp_role_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_ROLE_seq</param>
			</generator>
		</id>
		<property name="roleCode" column="role_code" type="java.lang.String" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="type" column="type" type="java.lang.String" />
		<property name="description" column="description" type="text" />
		<property name="language" column="language" type="java.lang.String" />

	</class>
</hibernate-mapping>
