<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpTeamReports" table="AMP_TEAM_REPORTS">
		<cache usage="read-write" />
		<id name="ampTeamReportsId" column="amp_team_reports_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="optimizer">none</param>
				<param name="increment_size">1</param>
				<param name="sequence_name">AMP_TEAM_REPORTS_seq</param>
			</generator>
		</id>
		<property name="teamView" column="team_view" type="java.lang.Boolean" />

		<many-to-one name="team" class="org.digijava.ampModule.aim.dbentity.AmpTeam" cascade="none" />
		<many-to-one name="report" class="org.digijava.ampModule.aim.dbentity.AmpReports" cascade="all" />

	</class>
</hibernate-mapping>

