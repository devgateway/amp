<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.aim.dbentity.AmpTemplatesVisibility"
		table="AMP_TEMPLATES_VISIBILITY" lazy="false">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">amp_templates_visibility_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="visible" column="visible" type="java.lang.String" />

		<set name="items" table="amp_modules_templates" lazy="false" sort="natural"	cascade="none">
			<cache usage="read-write" />
			<key column="template" />
			<many-to-many column="ampModule"
				class="org.digijava.ampModule.aim.dbentity.AmpModulesVisibility" />
		</set>

		<set name="features" table="amp_features_templates" lazy="false" sort="natural" cascade="none">
			<cache usage="read-write" />
			<key column="template" />
			<many-to-many column="feature"
				class="org.digijava.ampModule.aim.dbentity.AmpFeaturesVisibility" />
		</set>

		<set name="fields" table="amp_fields_templates" lazy="false" sort="natural" cascade="none"> 
			<cache usage="read-write" />
			<key column="template" />
			<many-to-many column="field"
				class="org.digijava.ampModule.aim.dbentity.AmpFieldsVisibility" />
		</set>

	</class>
</hibernate-mapping>
