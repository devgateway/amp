<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="org.digijava.ampModule.budget.dbentity.AmpBudgetSector"
		table="AMP_BUDGET_SECTOR">
		<cache usage="read-write" />
		<id name="idsector" column="budged_sector_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_BUDGET_SECTOR_seq</param>
			</generator>
		</id>

		<set name="organisations" table="AMP_ORGANISATION_BUDGETSECTOR" lazy="true">
			<key column="idsector" />
			<many-to-many class="org.digijava.ampModule.aim.dbentity.AmpOrganisation" column="amp_org_id" />
		</set>
		<property name="sectorname" column="sector_name" type="java.lang.String"/>
		<property name="code" column="code" type="java.lang.String"/>
	</class>

</hibernate-mapping>
