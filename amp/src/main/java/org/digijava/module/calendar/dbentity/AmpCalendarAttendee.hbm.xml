<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.calendar.dbentity.AmpCalendarAttendee"
		table="AMP_CALENDAR_ATTENDEE">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_CALENDAR_ATTENDEE_seq</param>
			</generator>
		</id>
		<many-to-one name="ampCalendar" column="CALENDAR_ID"
			class="org.digijava.ampModule.calendar.dbentity.AmpCalendar" />
		<many-to-one name="member" column="MEMBER_ID"
			class="org.digijava.ampModule.aim.dbentity.AmpTeamMember" />
		<many-to-one name="team" column="TEAM_ID" class="org.digijava.ampModule.aim.dbentity.AmpTeam" />
		<property name="guest" column="GUEST" type="java.lang.String" />
	</class>
</hibernate-mapping>
