<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.calendar.dbentity.AmpEventType" table="AMP_EVENT_TYPE">
		<id column="ID" name="id" type="java.lang.Long" unsaved-value="null">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">amp_event_type_seq</param>
			</generator>
		</id>
		<property name="name" column="NAME" type="java.lang.String" />
		<property name="color" column="COLOR" type="java.lang.String" />
	</class>
</hibernate-mapping>
