<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.contentrepository.dbentity.CrDocumentsToOrganisations"
		table="CR_DOCUMENTS_TO_ORGANISATIONS">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">CR_DOCUMENTS_TO_ORGANISATIONS_seq</param>
			</generator>
		</id>

		<many-to-one name="ampOrganisation" column="amp_organisation_id" lazy="no-proxy"
			class="org.digijava.ampModule.aim.dbentity.AmpOrganisation" cascade="none" />
		<property name="uuid" column="document_uuid" type="java.lang.String" index="uuid_index" />

	</class>
</hibernate-mapping>
