<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.ampModule.message.dbentity.AmpMessageReceiver" table="AMP_MESSAGE_RECEIVER">

		<id name="id" column="message_receiver_id" type="java.lang.Long">
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
				<param name="sequence_name">AMP_MESSAGE_RECEIVER_seq</param>
			</generator>
		</id>

		<many-to-one name="receiver" column="receiver_id"
			class="org.digijava.ampModule.aim.dbentity.AmpTeamMember" />
		<many-to-one name="message" column="message_id"
			class="org.digijava.ampModule.message.dbentity.AmpMessage" cascade="none" not-null="true" />

	</class>
</hibernate-mapping>