<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.MachineTranslationCharactersUsed" table="machine_translation_characters_used">
		<cache usage="read-write" />
		<id name="id" column="id" type="java.lang.Long">
			<generator class="assigned" />
		</id>
		<property access="field" name="month" type="java.lang.String" length="6" not-null="true" />
		<property access="field" name="used" type="java.lang.Integer" not-null="true" />
	</class>
</hibernate-mapping>
