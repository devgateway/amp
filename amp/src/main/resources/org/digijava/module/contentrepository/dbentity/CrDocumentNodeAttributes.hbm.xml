<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.contentrepository.dbentity.CrDocumentNodeAttributes"
		table="CR_DOCUMENT_NODE_ATTRIBUTES">
		<cache usage="read-write" />
		<id name="uuid" column="uuid" type="java.lang.String">
			<generator class="assigned"></generator>
		</id>

		<property name="publicVersionUUID" column="public_version_uuid" type="java.lang.String"
			index="public_version_uuid_index" />
		<property name="publicDocument" column="public_document" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>
