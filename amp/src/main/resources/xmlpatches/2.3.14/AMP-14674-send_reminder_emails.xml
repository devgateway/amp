<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
	xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
	<jira>AMP-14674</jira>
	<keyword>Job Manager</keyword>
	<keyword>Send reminder emails</keyword>
	<author>APopescu</author>
	<trigger type="all">
		<condition type="entryInTableMissing" tablename="amp_quartz_job_class"
			columnname="jc_name" columnvalue="Send Reminder Emails" />
	</trigger>
	<apply>
		<script>
			<lang delimiter=";" type="postgres">
				INSERT INTO amp_quartz_job_class (jc_id,jc_name, jc_class_fullname) VALUES(nextval('amp_quartz_job_class_seq'),'Send Reminder Emails', 'org.digijava.ampModule.message.jobs.SendReminderEmailJob');
			</lang>
		</script>
	</apply>

</tns:patch>