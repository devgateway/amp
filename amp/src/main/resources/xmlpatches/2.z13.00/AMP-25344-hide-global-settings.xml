<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
           xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
    <jira>AMP-25344</jira>
    <keyword>global settings</keyword>
    <author>jdeanquin</author>
    <description>Hide global settings</description>
    <apply>
        <script>
            <lang delimiter=";" type="postgres">
				UPDATE amp_global_settings SET section = 'resources' 
				WHERE settingsname IN ('Resource List Sort Column', 'Maximum File Size', 'Limit file type for upload');
            </lang>
        </script>
    </apply>
</tns:patch>
