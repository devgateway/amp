  "devDependencies": {
    "mocha": "~1.18.2",
    "event-stream": "~3.1.1",
    "mocha-lcov-reporter": "0.0.1",
    "istanbul": "~0.2.6",
    "coveralls": "~2.10.x"
  },
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/nfroidure/plexer/blob/master/LICENSE"
    }
  ],
  "bugs": {
