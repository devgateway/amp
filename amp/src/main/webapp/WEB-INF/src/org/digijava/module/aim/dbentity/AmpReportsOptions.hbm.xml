<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpReportsOptions" table="AMP_REPORTS_OPTIONS">
		<cache usage="read-write" />

		<id name="ampOptionId" column="amp_option_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_REPORTS_OPTIONS_seq</param>
			</generator>
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="options" column="options" type="java.lang.Character" />

	</class>
</hibernate-mapping>

