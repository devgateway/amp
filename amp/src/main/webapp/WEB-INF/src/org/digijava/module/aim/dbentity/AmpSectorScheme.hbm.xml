<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpSectorScheme" table="AMP_SECTOR_SCHEME">
		<cache usage="read-write" />
		<id name="ampSecSchemeId" column="amp_sec_scheme_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_SECTOR_SCHEME_seq</param>
			</generator>
		</id>

		<property name="secSchemeCode" column="sec_scheme_code" type="java.lang.String" />
		<property name="secSchemeName" column="sec_scheme_name" type="java.lang.String" />
        <property name="showInRMFilters" column="show_in_rm" type="java.lang.Boolean" />

	</class>
</hibernate-mapping>
