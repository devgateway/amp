<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.aim.dbentity.AmpSiteFlag" table="AMP_SITE_FLAGS">
		<id name="countryId" column="COUNTRY_ID" type="java.lang.Long" unsaved-value="null">
			<generator class="assigned" />
		</id>
		<property name="flag" column="FLAG" type="binary" length="50000"/>
		<property name="contentType" column="CONTENT_TYPE" type="java.lang.String" />

		<property name="defaultFlag" column="DEF_FLAG" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>

