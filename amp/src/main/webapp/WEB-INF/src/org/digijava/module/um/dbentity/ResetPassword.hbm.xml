<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.digijava.module.um.dbentity.ResetPassword" table="DG_RESET_PASSWORD" lazy="false">
		<id column="USER_ID" name="userId" type="long" unsaved-value="0">
			<generator class="assigned" />
		</id>
		<property column="RESET_DATE" length="400" name="resetDate" type="java.util.Date" />
		<property column="CODE" length="100" name="code" type="java.lang.String" />
	</class>
</hibernate-mapping>
