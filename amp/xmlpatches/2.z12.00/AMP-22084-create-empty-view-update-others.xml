<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
	xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
	<jira>AMP-22084</jira>
	<keyword>update potentially botched elements</keyword>
	<author>acartaleanu</author>
	<description>Modify broken column views table</description>
	<apply>
		<script>
			<lang delimiter=";" type="postgres">
				UPDATE amp_columns SET celltype = 'org.dgfoundation.amp.ar.cell.TextCell' WHERE celltype = 'TextCell';
			</lang>
		</script>
	</apply>
</tns:patch>