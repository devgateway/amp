<?xml version="1.0" encoding="UTF-8"?>
<tns:patch closeOnSuccess="true" retryOnFail="true"
           xmlns:tns="http://docs.ampdev.net/schemas/xmlpatcher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../../doc/xmlpatcher.xsd ">
    <jira>AMP-30433</jira>
    <keyword>Unique indicator name sector program</keyword>
    <author>drychter</author>
    <apply>
        <script>
            <lang delimiter=";" type="postgres">
                ALTER TABLE amp_category_value ADD CONSTRAINT amp_sector_indicator UNIQUE (amp_sector_id, indicator_id);
                ALTER TABLE amp_category_value ADD CONSTRAINT amp_program_indicator UNIQUE (amp_theme_id, indicator_id);
            </lang>
        </script>
    </apply>
</tns:patch>