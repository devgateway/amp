			<put-item name="tabHeader" module="aim" instance="default" teaser="mainProjectDetails"/>
			<put-item name="tabBody" module="aim" instance="default" teaser="blankTeaser"/>
	 		<put-item name="footer" layout="footerLayout"/>
		</layout>	
