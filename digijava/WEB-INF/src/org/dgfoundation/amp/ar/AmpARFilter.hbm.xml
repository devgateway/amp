<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="org.dgfoundation.amp.ar.AmpARFilter" table="AMP_AR_FILTER">
		<cache usage="read-write" />
		<id name="id" column="amp_filter_id" type="java.lang.Long">
			<generator class="native">
				<param name="sequence">AMP_AR_FILTER_seq</param>
			</generator>
		</id>
		<property name="ampStatusId" column="amp_status_id" type="java.lang.Long" />
		<property name="ampOrgId" column="amp_org_id" type="java.lang.Long" />
		<property name="ampSectorId" column="amp_sector_id" type="java.lang.Long" />
		<property name="region" column="region" type="java.lang.String" />
		<property name="ampModalityId" column="amp_modality_id" type="java.lang.Long" />
		<property name="ampCurrencyCode" column="amp_currency_code"
			type="java.lang.String" />
		<property name="fromYear" column="from_year" type="java.lang.Integer" />
		<property name="toYear" column="to_year" type="java.lang.Integer" />

<!--
		<set name="teams" table="AMP_AR_FILTER_TEAM" lazy="true">
			<cache usage="read-write" />
			<key column="id" />
			<many-to-many class="org.digijava.module.aim.dbentity.AmpTeam"
				column="amp_team_id" />
		</set>
-->
	</class>
</hibernate-mapping>
