<table class="bluebook">
	<tr>
		<td>
			<b><%= ll(:bluebook, :pages, :contents, :eu_cooperation, :by_toa_in, :year => year, :forecasts => year + 1)%></b>
			<div id="column_graph">
				<strong>You need to upgrade your Flash Player</strong>
			</div>
			<!-- saved from url=(0013)about:internet -->
			<!-- amcolumn script-->
		  <script type="text/javascript">
			// <![CDATA[		
					var so = new SWFObject("/amcolumn.swf", "amcolumn", "1150", "430", "8", "#DBE2F1");
					so.addVariable("path", "/");
					so.addVariable("settings_file", escape("/bluebook/<%= year %>/charts/settings/cooperation_by_type_of_aid.xml"));
					so.addVariable("data_file", escape("/bluebook/<%= year %>/charts/eu_cooperation_type_of_aid_data.xml"));  
			    	so.addVariable("preloader_color", "#999999");
					so.write("column_graph");
					// ]]>
				</script>
			<!-- end of amcolumn script -->
			<br><br><font size=1><%= ll(:terms, :source) %>: 2008 ODAnic - www.odadata.eu/odanic</font>
		</td>
	</tr>
</table>

