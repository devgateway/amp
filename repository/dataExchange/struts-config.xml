<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="deExportForm" type="org.digijava.module.dataExchange.form.ExportForm"/>
	</form-beans>
	<action-mappings>
		<!-- Export routine -->
    	<action path="/dataExchange/exportWizard" forward="/showLayout.do?layout=exportWizard" />
		<action path="/dataExchange/export" scope="session" type="org.digijava.module.dataExchange.action.ExportAction" name="deExportForm" validate="false" parameter="method">
			<forward name="default" path="/showLayout.do?layout=exportWizard"/>
		</action> 
	</action-mappings>
</struts-config>
