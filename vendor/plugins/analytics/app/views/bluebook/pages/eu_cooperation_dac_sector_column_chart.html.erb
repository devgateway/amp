<table class="bluebook">
	<tr>
		<td>
			<b><%= ll(:bluebook, :pages, :contents, :eu_cooperation, :by_sector_in, :year => year ) %> </b>
			<br>
			<br>
			<%= ll(:bluebook, :chart_subtitles, :by_sector_in) %> <%= year %>
			<div id="multicolumn_graph">
				<strong>You need to upgrade your Flash Player</strong>
			</div>
			<!-- saved from url=(0013)about:internet -->
			<!-- amcolumn script-->
		  <script type="text/javascript">
			// <![CDATA[		
					var so = new SWFObject("/amcolumn.swf", "amcolumn", "1000", "1200", "8", "#DBE2F1");
					so.addParam("wmode", "transparent");
					so.addVariable("path", "/");
					so.addVariable("settings_file", escape("/bluebook/<%= year %>/charts/settings/dac_sector_column_data_settings.xml"));
					so.addVariable("data_file", escape("/bluebook/<%= year %>/charts/dac_sectors_column_data.xml"));  
			    	so.addVariable("preloader_color", "#999999");
					so.write("multicolumn_graph");
					// ]]>
				</script>
			<!-- end of amcolumn script -->
			<br><br><font size=1><%= ll(:terms, :source) %>: 2008 ODAnic - www.odadata.eu/odanic</font>
		</td>
	</tr>
</table>