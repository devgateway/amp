<table width=740 class="admin">
	<tr>
		<td class="head2"><center><%= ll(:bluebook, :title) %> - <%= ll(:countries, :nicaragua) %></center></td>
	</tr>
	<tr>
		<td class="title"><%= lc(:toc) %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:foreword), { :action => "foreword" } %></td>
	</tr>
	<tr>
		<td class="sub_title"><%= lc(:eu_at_a_glance, :title) %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :building_europe), { :action => "eu_at_a_glance_50_years" } %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :member_states) , { :action => "eu_at_a_glance_member_states" } %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :institutions), { :action => "eu_at_a_glance_institutions" } %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :consensus), { :action => "eu_at_a_glance_consensus_on_development" } %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :code_of_conduct), { :action => "eu_at_a_glance_code_of_conduct" } %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_at_a_glance, :eu_aid), { :action => "eu_at_a_glance_aid" } %></td>
	</tr>
	
	<tr>
		<td class="sub_title"><%= lc(:eu_cooperation, :policy) %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :process), 
					{ :action => "eu_at_a_glance_h_and_a_process" } %>
		</td>
	</tr>
	<tr>
		<td class="sub_title"><%= lc(:eu_cooperation, :facts) %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :achievement_in, :year => 2007), 
					{ :action => "eu_cooperation_achievements"}, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :forecasts_for, :year => 2008), 
					{ :action => "eu_cooperation_forecasts" }, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :trend, :from => 2007, :to => 2008), 
					{ :action => "eu_cooperation_trend" }, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :by_sector_in, :year => 2007), 
					{ :action => "eu_cooperation_dac_sector_column_chart" }, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :by_region_in, :year => 2007), 
					{ :action => "eu_cooperation_region_column_chart" } %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :by_toa_in, :year => 2007, :forecasts => 2008), 
					{ :action => "eu_cooperation_type_of_aid" }, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td><%= link_to lc(:eu_cooperation, :in_percent_by_toa, :year => 2007), 
					{ :action => "eu_cooperation_type_of_aid" }, 
					:popup => ['new_window', 'height=700,width=800,scrollbars=yes,resizable=yes'] %>
		</td>
	</tr>
	<tr>
		<td class="sub_title"><%= lc(:profiles, :title) %></td>
	</tr>
	
	<% Donor.main.all(:order => "name ASC").each do |d| %>
		<tr>
			<td><a href="/bluebook/donor_profile/<%= d.id %>"><%= d.name.titleize %></a></td>
		</tr>
	<% end %>
	<tr>
		<td class="sub_title"><%= lc(:note, :title) %></td>
	</tr>
	<tr>
		<td><%= link_to lc(:note, :explanatory_note), { :action => "explanatory_note" } %></td>
	</tr>
</table>