	xsi:schemaLocation="http://docs.ampdev.net/schemas/xmlpatcher ../doc/xmlpatcher.xsd ">
	<jira>AMP-8295</jira>
	<keyword>Flags</keyword>
	<description>
		The primary key shouldn't be auto_incremented
  	</description>
	<apply>
		<script>
			<lang delimiter=";" type="mysql">
			ALTER TABLE  `amp_site_flags` CHANGE  `COUNTRY_ID`  `COUNTRY_ID` BIGINT( 20 ) NOT NULL;
			</lang>
		</script>
	</apply>
</tns:patch>

